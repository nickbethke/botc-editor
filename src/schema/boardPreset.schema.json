{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Board Preset Schema",
	"additionalProperties": false,
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"width": {
			"type": "number",
			"minimum": 1,
			"maximum": 20
		},
		"height": {
			"type": "number",
			"minimum": 1,
			"maximum": 20
		},
		"data": {
			"type": "object"
		}
	},
	"required": ["data", "name", "width", "height"]
}
