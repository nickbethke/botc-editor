{
	"$schema": "http://json-schema.org/draft-07/schema",
	"additionalProperties": false,
	"$id": "Board Preset Schema",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"width": {
			"type": "number",
			"minimum": 1,
			"maximum": 20
		},
		"height": {
			"type": "number",
			"minimum": 1,
			"maximum": 20
		},
		"data": {
			"type": "object"
		}
	},
	"required": [
		"data",
		"name",
		"width",
		"height"
	]
}
